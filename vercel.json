{
  "env": {
    "GEMINI_API_KEY": "@gemini_api_key",
    "OPENAI_API_KEY": "@openai_api_key",
    "ANTHROPIC_API_KEY": "@anthropic_api_key",
    "GROQ_API_KEY": "@groq_api_key",
    "OLLAMA_URL": "@ollama_url",
    "HF_API_KEY": "@hf_api_key"
  },
  "functions": {
    "api/chat.js": {
      "memory": 512,
      "maxDuration": 30
    },
    "api/models.js": {
      "memory": 256,
      "maxDuration": 10
    },
    "api/embeddings.js": {
      "memory": 512,
      "maxDuration": 30
    }
  },
  "builds": [
    {
      "src": "api/**.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/$1.js"
    }
  ]
}
